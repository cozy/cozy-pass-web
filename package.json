{
  "name": "cozy-pass-web",
  "productName": "Cozy Pass",
  "description": "A secure and free password manager for all of your devices.",
  "version": "0.0.0",
  "keywords": [
    "cozy",
    "bitwarden",
    "password",
    "vault",
    "password manager"
  ],
  "author": "Cozy Cloud <contact@cozycloud.cc> (https://cozy.io/)",
  "homepage": "https://github.com/cozy/cozy-pass-web#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cozy/cozy-pass-web.git"
  },
  "bugs": {
    "url": "https://github.com/cozy/cozy-pass-web/issues"
  },
  "license": "GPL-3.0",
  "scripts": {
    "start": "npm run build:browser:watch",
    "sub:init": "git submodule update --init --recursive",
    "sub:update": "git submodule update --remote",
    "sub:pull": "git submodule foreach git pull origin master",
    "sub:commit": "npm run sub:pull && git commit -am \"update submodule\"",
    "postinstall": "./node_modules/.bin/electron-rebuild && npm run sub:init && patch-package",
    "lint": "tslint 'src/**/*.ts'",
    "lint:fix": "tslint 'src/**/*.ts' --fix",
    "build:browser:watch": "gulp prebuild:renderer && cross-env DEVELOPMENT=1 webpack --config webpack.browser.js --watch",
    "build:browser:prod": "gulp prebuild:renderer && cross-env DEVELOPMENT=0 webpack --config webpack.browser.js",
    "build:browser:server": "gulp prebuild:renderer && cross-env DEVELOPMENT=1 webpack serve --config webpack.browser.js",
    "build:browser:server:analysebundle": "gulp prebuild:renderer && cross-env BUNDLE_ANALYSE=1 webpack-dev-server --config webpack.browser.js --watch",
    "cozyPublish": "cozy-app-publish --token $REGISTRY_TOKEN --prepublish downcloud --postpublish mattermost --space default --build-dir build-browser"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^11.2.10",
